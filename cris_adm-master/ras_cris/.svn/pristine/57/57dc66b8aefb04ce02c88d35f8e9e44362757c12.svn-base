.button.small.icon .save {background-position: -169px -418px;}
.button.small.icon .sch2 {background-position: -205px -416px;}

/* Button Hover */
.button *:hover,
.button *:active,
.button *:focus{ }

/* Button Strong */
.button.strong *{ font-weight:bold !important;}

