.sgnb {position:relative; width:100%; height:31px; margin: 0 auto; padding:0px; background-image:url(../../../../images/ncmik/ptl/include/gnb/bg_gnb2.gif); background-attachment:scroll; background-repeat:repeat-x; background-position-x:left; background-position-y:top; background-color:transparent; clear:both; margin:0 0 0 0;}
.sgnb div {list-style: none; width:969px; height:31px; overflow: hidden; margin-top:0px; margin-right: auto; margin-bottom: 0px; margin-left: auto;}
.sgnb div ul {list-style: none; width:969px; height:31px; overflow: hidden; margin-top:0px; margin-right: 0px; margin-bottom: 0px; margin-left: 0px;}
.sgnb div ul li {font-family:"굴림",Gulim; font-size:12px; float: left; color: #fff; width:161px; height:31px; text-align: center; font-weight: bold; padding-top:9px;}
.sgnb div ul .topBtn{cursor: pointer;}
.sgnb div ul .focusBtn{background-color: 4d81bd;}
.sgnb div ul .topBtnBlank{cursor: arrow;}

.sgnb div ul.gnb li ul.top_sub_menu {
	display:none;
	position:absolute;
	list-style: none;
	width:300px;
	height: 160px;
	overflow: hidden;
	margin-top: 8px;
	margin-right: 0px;
	margin-bottom: 0px;
	margin-left: 0px;
}
.sgnb div ul.gnb li:hover ul.top_sub_menu {
	display:block;
}
.sgnb div ul.gnb li ul.top_sub_menu li {
	font-size:12px;
	float: left;
	color: #000;
	width:151px;
	height:25px;
	line-height:25px;
	text-align: left;
	font-weight: normal;
	background:#ebebeb;
	padding-top:0px;
	padding-left:4px;
	margin-top:0px;
    border-left:5px solid rgb(13, 55, 129);
    border-bottom:1px solid #ddd;
}
.sgnb div ul.gnb li ul.top_sub_menu li a {
	display:block;
	width:100%;
	height:100%;
	font-weight:normal;
}
.sgnb div ul.gnb li ul.top_sub_menu li:hover {
	background:#ddd;
}
.sgnb div ul.gnb li ul.top_sub_menu li:hover a {
	color: #CC0000;
}

.sgnb div ul.gnb li ul.top_sub_menu li.top_sub_menu_li:hover ul.sub_menu {display:block;position:relative;width:151px;height:300px;top:-25px;right:-151px;z-index:9999999}
.sgnb div ul.gnb li ul.top_sub_menu li.top_sub_menu_li ul.sub_menu {display:none;}
.sgnb div ul.gnb li ul.top_sub_menu li.top_sub_menu_li ul.sub_menu li {
	font-size:12px;
	float: left;
	color: #000;
	height:25px;
	line-height:25px;
	text-align: left;
	font-weight: normal;
	background:#ebebeb;
	padding-top:0px;
	padding-left:4px;
	margin-top:0px;
    border-top:0.1px solid #aaa;
	border-left:2px solid rgb(13, 55, 129);
    border-bottom:0.1px solid #aaa;
}
.sgnb div ul.gnb li ul.top_sub_menu li.top_sub_menu_li ul.sub_menu li a {
	display:block;
	color: #000;
	width:100%;
	height:100%;
	font-weight:normal;
}
.sgnb div ul.gnb li ul.top_sub_menu li.top_sub_menu_li ul.sub_menu li:hover {
	background:#ddd;
}
.sgnb div ul.gnb li ul.top_sub_menu li.top_sub_menu_li ul.sub_menu li:hover a {
	color: #CC0000;
}











