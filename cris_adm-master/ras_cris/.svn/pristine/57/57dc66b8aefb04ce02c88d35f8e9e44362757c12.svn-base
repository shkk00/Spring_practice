.tableFixed {table-layout: fixed; word-wrap: break-word;}

.grid, .grid table {width:100%; margin:10px 0; empty-cells:show; border-collapse:collapse; border-style:none; border-width:0;}
.grid thead th, .grid thead td, .grid tbody th, .grid tbody td, .grid tfoot th {border-style:none; border:0; border-width:0; empty-cells:show; vertical-align: middle;;}
.grid thead th, .grid thead td {color:#666; padding: 8px 5px 5px;}
.grid tfoot th, .grid tfoot td {background: #F5F9FA; border: 1px solid #d8d8d8; color: #666; padding: 2px;}
.grid thead th {
	background: #ffffff url(../../../../images/ncmik/ptl/cmm/board/contents/grid_top.gif) repeat-x bottom left; border-top: 2px solid #00006a; border-bottom: 1px solid #cbcbcb; font-size: 12px; line-height: 1.3; word-wrap:break-word; color: #333;
}
.grid thead th.top2 {
	background: #ffffff url(../../../../images/ncmik/ptl/cmm/board/contents/grid_top2.gif) repeat-x bottom left;;
}

.grid tbody th, .grid thead td {border-top:1px solid #cbcbcb; border-bottom:1px solid #cbcbcb; padding: 6px 10px; color: #666; background: #f9f9f9; font-size: 12px; font-weight:normal; text-align: center; line-height: 1.3;}
.grid tbody th label.necessary {background: url(../../../../images/ncmik/ptl/cmm/board/common/necessary.gif) no-repeat 98% 2px !important; padding-right: 10px !important;}/* 필수 */
.grid tbody td thead th {background: #71bfeb url(/right4me../../../../images/ncmik/ptl/cmm/board/2012/content/grid_th.gif) repeat-x 0 0; border: 1px solid #3f9ecf; font-size: 11px; line-height: 1.5; word-wrap:break-word; color: #666 !important; text-align: center;}
.grid tbody td th {border: 1px solid #e5e5e5; padding: 3px 5px; color: #666 !important; background: #f9fafa; font-size: 11px;}
.grid tbody td th label {font-weight: bold !important; color: #777 !important; }
.grid tbody td td {font-size: 11px !important;}
