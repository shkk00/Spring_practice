@media all and (max-width:502px) {
	.headerWrap{
		height:50px;
		transition:all 0.3s;
		-webkit-transition:all 0.3s;	
	}
	.link_btn_wrap{
 		/* right:36px; */
 		right:24px;
 		top:-20px;
 	}
 	.link_btn_wrap .link_btn{
 		/* padding:2px 9px; */
 			padding: 0px 9px;
 	}
	#myCarousel>.carousel-inner {
	    background: url(../../images/main/main_image2.png) no-repeat;
	    background-position:78% center;
	    background-size:cover;
	}
	ul.gnb1{
		display:none;	
	}
	.menu_div{
		height:50px;
		position:fixed;
		width:100%;
		top:0;
		z-index:1000;
		background:#fff;
	}
	#nav_gnb{
		height:50px;
		border-bottom: 1px solid rgba(51,122,183,0.5);
	}
	
	#under_bar{
		top:50px;
	}
