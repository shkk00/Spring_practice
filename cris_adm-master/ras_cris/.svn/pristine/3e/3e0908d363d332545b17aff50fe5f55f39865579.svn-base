	 	-o-transform:translate(-50%, 0);
	 	-ms-transform:translate(-50%, 0);
	 	-moz-transform:translate(-50%, 0);
	 	-webkit-transform:translate(-50%, 0);
  	}
	ul.gnb2{
		width:20%;
		max-width:200px;
	}

}
@media all and (max-width:1100px) {
	.fSite_select{
		right:20px;
	}
}
@media all and (max-width:990px) {

}

@media all and (max-width:920px) {
	.mainsearchbox{
		display:inline-block;
		width:100%;
		max-width:536px;
		position:relative;
		transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
 	.webdesigntuts-workshop input{	 	
		min-width:300px;
		position:relative;
		transition:all 0.3s;
		-webkit-transition:all 0.3s;
		
	} 
	.webdesigntuts-workshop button{
		transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
	#noticnpop{
		display:block;
		height:auto;
	}
	#notic_body {
		width: 65%;
	    transition: all 0.5s;
	    -webkit-transition: all 0.5s;
    	margin:0 auto;
    	float:none;
    	display:block;
    	margin-bottom:10px;
    	margin:15px auto;
    	transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
	#notic_body:nth-child(1){
		margin-left:auto;
	}
	
	#brd_list1, #brd_list2, #brd_list3{
		max-width:95%;
		margin:0 auto;
	}

	#footer div.footLinkWrap div.footLink .link > div {
	    padding: 10px 30px 0px 0px;
	    transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
	#footer div.footLinkWrap div.footLink .link > div:first-child{
		padding-left:30px;
		 transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
	#rollBanner{
		display:none;
	}
	#noticnpop{
		margin-bottom:50px;
	}
}
@media all and (max-width:820px) {
	#footer div.footLinkWrap div.footLink{
  		height:auto;
  		padding:5px 0;
  		 transition:all 0.3s;
		-webkit-transition:all 0.3s;
  	}
 }
@media all and (max-width:768px) {
	
	#notic_body {
		width: 90%;
	    transition: all 0.5s;
	    -webkit-transition: all 0.5s;
    	margin:0 auto;
    	float:none;
    	display:block;
    	margin-bottom:10px;
    	margin:15px auto;
	}
	.Notice li ul li{
	    border-bottom: 1px dashed #ccc;
	}
	#footer div.foot > div{
		width:100%;
		text-align:center;
	}
	#footer div.foot > div > div{
  		width:100%;
  		position:relative;
  		display:block;
  		text-align:center;
  		padding:5px 0;
	}
	/* select-box width사이즈 */
	.select-width{
		width: 25%;
	}	
}

 @media all and (max-width:640px) {
 	.top_login{
 		display:none;
		transition:all 0.3s;
		-webkit-transition:all 0.3s; 
	}
 	#header{
 		padding:0;
 	}
 	#logo{
		position:absolute;
		left:0;
		top:0;
	}
 	#nav_gnb{
 		height:115px;
 	}
 	.link_btn_wrap{
 		left:auto;
 		right:44px;
 		top:-18px;
 	}
 	.link_btn_wrap .link_btn{
 		font-size:12px;
 		font-weight:300;
 		padding:0px 9px;
 	}
 	.link_btn_wrap .link_btn:first-child{
 		width:auto;
 	}
 	.navbar-wrapper .navbar{
 		margin:20px 0 0 0;
 	}
 	.webdesigntuts-workshop input{	 	
 		width:70%;
		position:absolute;
		right:45px;
		transition:all 0.3s;
		-webkit-transition:all 0.3s;
	    height: 38px;
	} 
	.webdesigntuts-workshop button{
		float:right;
		height:38px;
		width:38px;
		transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
	.navbar-wrapper .navbar{
	    top: 0;
	    left: 0;
   		transform: translate(0,0);
   		-o-transform: translate(0,0);
   		-ms-transform: translate(0,0);
   		-moz-transform: translate(0,0);
   		-webkit-transform: translate(0,0);
   		transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
	.webdesigntuts-workshop{
		margin-top:0;
		transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
	.webdesigntuts-workshop>.help{
		display:none;
		transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
	.mainsearchbox {
	    background: rgba(0,0,0,0.0);
	    border: 0px solid rgba(0,0,0,0.15);
	    padding: 0;
	    padding-bottom: 0;
	    transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
	.mainsearchbox>.help{
		display:none;
		transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
	.webdesigntuts-workshop input{
		border:1px solid #ccc;
		transition:all 0.3s;
		-webkit-transition:all 0.3s;
		font-size:13px;
	}
	.mainsearchbox button{
		font-size:12px;
	}
	
	#Round_box2{
		top:150px;
		transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
	#footer div.footLinkWrap div.footLink .link{
		padding-left:50px;
	}
}

@media all and (max-width:600px) {
	.webdesigntuts-workshop input{	 	
 		min-width:220px;
	} 	
}
@media all and (max-width:520px) {
