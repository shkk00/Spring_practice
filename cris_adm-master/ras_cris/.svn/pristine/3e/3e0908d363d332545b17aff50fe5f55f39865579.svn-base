/* **************** media start ********** */
@media all and (max-width:1200px) {
	#header {
    	width:100%;
    	position:relative;
    	transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
	#logo{
		width:17%;
	    min-width:218px;
	    transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
	.top_login{
		padding-right:10px;
	}
	div.mesh{
		right:10px;
	}
  	.navbar-wrapper {
	    width: 100%;
	    position:absolute;
	    left:0;
	    transition:all 0.3s;
		-webkit-transition:all 0.3s;
  	}
  	#container-wrap {
	  	width: 100%;
	  	transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
  	.container{
		width:100%;
		transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
/*   	.webdesigntuts-workshop{
  		width:100%;
  		position:absolute;
  		left:50%;
  		transform:translate(-50%, 0);   
	 	-o-transform:translate(-50%, 0);
	 	-ms-transform:translate(-50%, 0);
