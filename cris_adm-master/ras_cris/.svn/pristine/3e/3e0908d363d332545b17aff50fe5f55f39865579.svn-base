	    /* display: block; */
		left:55%;
		transition:left 0.5s;
		-webkit-transition:left 0.5s;
	}
	.curtain.on{
		display:block;
	}
	#myCarousel>.carousel-inner{
		height:255px;
	}
	
	#Round0001, #Round0002, #Round0003, #Round0004{
		width:40%;		
		transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
	#Round_box2 {
	    height: 270px;
	    top: 10px;
	    z-index: 2;
	    transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
	#Round_box2>div{
		width:80%;
		display:block;
		margin:0 auto;    
	}   
	#Round0001{ width:40%;	left:12%; }
	#Round0002{ width:40%;	left:8%; }
	#Round0003{ width:40%;	left:12%; }
	#Round0004{ width:40%;	left:8%; }
	

	#Round0003{ margin-top:-4%; }
	#Round0004{ margin-top:-4%; }
	  
	
	/*호버되는이미지*/
	#Round0001:hover .overlay {
	  display:none !important;
	}
	#Round0002:hover .overlay {
	  display:none !important;
	}
	#Round0003:hover .overlay {
	  display:none !important;
	}
	#Round0004:hover .overlay {
	  display:none !important;
	}
	
	/* notic_body */
	.Notice{
		padding:10px;
		position:relative;
		border: 1px solid #E7E7E7;
		box-shadow:3px 3px 5px rgba(0,0,0,0.2);
		-o-box-shadow:3px 3px 5px rgba(0,0,0,0.2);
		-ms-box-shadow:3px 3px 5px rgba(0,0,0,0.2);
		-moz-box-shadow:3px 3px 5px rgba(0,0,0,0.2);
		-webkit-box-shadow:3px 3px 5px rgba(0,0,0,0.2);
	}
	.boardTab{
		font-size:17px;
	}
	.plus>a {
    width: 20px;
    height: 20px;
	}
	.plus>a>span {
    	font-size: 27px;
	}
	.Notice li ul li a{
		font-size:13px;
		padding: 5px 0px;
		margin:0px;
	}
	.Notice>ul>li>ul>li:last-child{
		border-bottom:0;
	}
	.plus {
    top: 10px;
    right: 10px;
	}
	.t2 {
    	font-size: 12px;
	}
	.mainsearchbox{
		width:245px;
	}
	.webdesigntuts-workshop{
		display:inline-block;
		text-align:center;
		left:50%;
		margin:0;
		transform:translate(-50%, 0);   
	 	-o-transform:translate(-50%, 0);
	 	-ms-transform:translate(-50%, 0);
	 	-moz-transform:translate(-50%, 0);
	 	-webkit-transform:translate(-50%, 0);
	 	transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
	.webdesigntuts-workshop input{
		left:0;
		min-width:208px;
		transition:all 0.3s;
		-webkit-transition:all 0.3s;
		float:left;
	}
	.webdesigntuts-workshop button {
	    margin:0;
	    width: 30px;
	    height: 30px;
	    margin-left: 7px;
	    float:right;
	}
	#logo{
		position:absolute;
		left:50%;
		transform:translate(-50%, 0);   
	 	-o-transform:translate(-50%, 0);
	 	-ms-transform:translate(-50%, 0);
	 	-moz-transform:translate(-50%, 0);
	 	-webkit-transform:translate(-50%, 0);
	 	transition:all 0.3s;
		-webkit-transition:all 0.3s;
		width:13%;
		margin:0;
	}
	#footer .english_page{
		display:block;
	}
	 #footer div.footLinkWrap div.footLink .link > div {
	    padding: 5px 0;
	    float:none;
	    transition:all 0.3s;
		-webkit-transition:all 0.3s;
	}
	#footer div.footLinkWrap div.footLink div.fSite{
  		position:relative;
  		 transition:all 0.3s;
		-webkit-transition:all 0.3s;
  	}
  	#footer div.footLinkWrap div.footLink .link > div:first-child{
