div.jqcloud span.w10 { font-size: 300%; }
div.jqcloud span.w9 { font-size: 270%; }
div.jqcloud span.w8 { font-size: 240%; }
div.jqcloud span.w7 { font-size: 210%; }
div.jqcloud span.w6 { font-size: 180%; }
div.jqcloud span.w5 { font-size: 160%; }
div.jqcloud span.w4 { font-size: 140%; }
div.jqcloud span.w3 { font-size: 120%; }
div.jqcloud span.w2 { font-size: 100%; }
div.jqcloud span.w1 { font-size: 80%; }

/* colors */

div.jqcloud { color: #09f; }
div.jqcloud a { color: inherit; }
div.jqcloud a:hover { color: #0064CD; }
div.jqcloud a:hover { color: #66CDAA; }
div.jqcloud span.w10 { color: #0064CD; }
div.jqcloud span.w9 { color: #46B4B4; }
div.jqcloud span.w8 { color: #60BD89; }
div.jqcloud span.w7 { color: #39d; }
div.jqcloud span.w6 { color: #90c5f0; }
div.jqcloud span.w5 { color: #90a0dd; }
div.jqcloud span.w4 { color: #7E6ECD; }
div.jqcloud span.w3 { color: #a0ddff; }
div.jqcloud span.w2 { color: #66CDAA; }
div.jqcloud span.w1 { color: #aab5f0; }

/* layout */     

div.jqcloud {
  overflow: hidden;
  position: relative;
}

div.jqcloud span { padding: 0; }